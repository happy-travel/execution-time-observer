<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Nullable>enable</Nullable>
    <LangVersion>9</LangVersion>
    <Copyright>HappyTravel.com</Copyright>
    <Authors>Denis Nikolaev</Authors>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NeutralLanguage>en</NeutralLanguage>
    <Description>Sending notification if function running longer than time</Description>
    <PackageProjectUrl>https://github.com/happy-travel/stdout-logger</PackageProjectUrl>
    <RepositoryURL>https://github.com/happy-travel/execution-time-observer</RepositoryURL>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>0.8</Version>
  </PropertyGroup>

</Project>
